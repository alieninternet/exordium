## Tell kine what we're all about.. We only need to specify the language code
.LangCode = en



##
## Stuff that is used by exordium's core
##

# could not load module, generic (0 = module name, 1 = error)
exordium_core_MODULE_ERROR_GENERIC = Could not load %p0: %p1

# module has no init function (0 = module name)
exordium_core_MODULE_ERROR_NOINIT = Could not load %p0: Module does not contain an initialisation function

# module failed to initialise (0 = module name)
exordium_core_MODULE_ERROR_INITFAIL = Could not load %p0: Module failed to initialise

# module loaded (0 = name, 1 = version)
exordium_core_MODULE_LOADED = Loaded module '%p0' version %p1


##
## Stuff common to all modules
##

# Unknown command (0 = command given, 1 = module name)
exordium_common_UNKNOWN_COMMAND = Unknown command (%p0). Please type \b/msg %p1 help\b for more details

# Unknown command, fuzzy logic? (yes, an innovative idea from PICKLE ;) (0 = command, 1 = module name, 2 = guess command)
exordium_common_UNKNOWN_COMMAND_FUZZY = Unknown command (%p0), did you mean '%p2'? Please type \b/msg %p1 help\b for more details


##
## mod_nick
##

# Nick already registered (0 = nickname)
exordium_mod_nick_ERROR_NICK_REGISTERED = That nickname (%p0) is already registered!
